
bool isPalindrome(int x){
        long res=0,X;
        X=x;
    
    if(x<0)
        return false;
    else if (x>=0 && x<10)
        return true;
    
    while(x)
    {
        res=res*10+x%10;
        x/=10;
           
    }
    
    if(res == X)
            return true; 
    else
            return false;


}


