class Solution(object):
    def simplifyPath(self, path):
        """
        :type path: str
        :rtype: str
         """
        
        Stack =[];
        
        p = path.split('/');
        
        for i in p :
            if (len(Stack)>0 and i == ".."):
                Stack.pop();
            elif ( i!= '' and i!= '..' and i != '.'):
                Stack.append(i);
        
        return '/'+'/'.join(Stack);
            
